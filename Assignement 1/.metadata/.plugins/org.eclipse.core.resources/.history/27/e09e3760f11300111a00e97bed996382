#include <iostream>
using namespace std;
class ToolBooth{
	int totalVehicles;
	double totalRevenue;

	void reset(int totVehicles,double totRevenue){
		totalVehicles=totVehicles;
		totalRevenue=totRevenue;
	}

	void vehiclePayingToll(int vehicleType,double tollAmount){

	}

	int getTotalVehicles()const{
		return totalVehicles;
	}

	double getTotalRevenue(){
		return totalRevenue;
	}
};
int main() {
	int choice;
	do{
		cout<<"MENU DRIVEN PROGRAM!";
		cout<<"1. Add a standard car and collect toll ";
		cout<<"2. Add a truck and collect toll ";
		cout<<"3. Add a bus and collect toll ";
		cout<<"1. Add a standard car and collect toll ";
	}while(choice!=7);
	return 0;
}
