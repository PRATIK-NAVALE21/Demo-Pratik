#include <iostream>
using namespace std;
class Student{
	string name;
	int rollNumber;
	float marks;
	char grade;

	string getName()const{
			return name;
	}

	void setName(string Studentname){
			name=Studentname;
	}

	int getRollNumber()const{
		return rollNumber;
	}

	void setRollNumber (int Number){
		if(Number>0){
			rollNumber=Number;
		}

	}

	void  setMarks(int Studentmarks){
		if(Studentmarks>0){
			marks=Studentmarks;
		}
	}

	int getMarks()const{
		return marks;
	}

	void setGrade(char StudentGrade){
		grade=StudentGrade;
	}

	int getGrade() const{
		return grade;
	}

};
int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
